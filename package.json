{
  "name": "@qiuqfang/webpack-plugin-pages",
  "version": "1.0.11",
  "private": false,
  "description": "This is a plugin that automatically generates react routes",
  "keywords": [
    "webpack",
    "plugin",
    "react",
    "pages"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && tsc -p tsconfig.json",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz",
    "test": "jest",
    "bumpp": "bumpp",
    "release": "npm run build && npm publish --access public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "author": "Quincy Qiu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@jest/globals": "^29.6.2",
    "@qiuqfang/eslint-config": "^0.0.12",
    "@qiuqfang/rollup-plugin-swc": "^0.0.12",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^18.17.4",
    "@types/react": "^18.2.20",
    "@types/webpack-env": "^1.18.1",
    "bumpp": "^9.1.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "react": "^18.2.0",
    "rollup": "^4.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "chokidar": ">=^3.5.3",
    "prettier": ">=^2.8.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.*": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "prettier": "^3.0.1"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  }
}
